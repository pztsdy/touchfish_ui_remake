{
  "name": "touchfish-ui-remake",
  "version": "1.4.0",
  "description": "A beautiful, modern touchfish client built with Electron.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "keywords": [
    "Electron",
    "chat",
    "touchfish"
  ],
  "author": {
    "name": "Piaoztsdy",
    "email": "piaoztsdy@outlook.com"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.0"
  },
  "devDependencies": {
    "electron-builder": "^26.0.12",
    "electron": "^26.0.0"
  },
  "build": {
    "appId": "com.piaoztsdy.touchfish_ui_remake",
    "productName": "Touchfish UI Remake",
    "directories": {
      "output": "build"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "installerIcon": "tchui.ico",
      "uninstallerIcon": "tchui.ico",
      "uninstallDisplayName": "Uninstall Touchfish UI Remake",
      "license": "license.txt",
      "deleteAppDataOnUninstall": true
    },
    "icon": "tchui.ico"
  }
}