{
  "name": "touchfish-ui-remake",
  "version": "1.5.0-pre",
  "description": "A beautiful, modern touchfish client built with Electron.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "keywords": [
    "Electron",
    "chat",
    "touchfish"
  ],
  "author": {
    "name": "Piaoztsdy",
    "email": "piaoztsdy@outlook.com"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.0",
    "dompurify": "^3.2.6",
    "github-markdown-css": "^5.8.1",
    "jsdom": "^26.1.0",
    "markdown-palettes": "^0.4.13",
    "marked": "^16.2.1"
  },
  "devDependencies": {
    "electron": "^31.7.7",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "appId": "com.piaoztsdy.touchfish_ui_remake",
    "productName": "Touchfish UI Remake",
    "directories": {
      "output": "build"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "installerIcon": "tchui.ico",
      "uninstallerIcon": "tchui.ico",
      "uninstallDisplayName": "Uninstall Touchfish UI Remake",
      "license": "license.txt",
      "deleteAppDataOnUninstall": true
    },
    "icon": "tchui.ico"
  }
}
